{
    "wikit-InlineProgressBar": {
        "background-color": {
            "value": "{background.color.progressive.default.value}"
        },
        "height": {
            "value": "{dimension.static.size.50.value}"
        },
        "animation": {
            "duration": {
                "value": "{animation.loader.duration.slow.value}"
            }
        },
        "track": {
            "background-color": {
                "value": "{background.color.base.transparent.value}"
            },
            "height": {
                "value": "{dimension.static.size.50.value}"
            },
            "width": {
                "value": "{dimension.width.full.value}"
            }
        },
        "determinate": {
            "border-radius": {
                "value": "{border.radius.none.value}"
            }
        },
        "indeterminate": {
            "border-radius": {
                "value": "{border.radius.rounder.value}"
            },
            "width": {
                "value": "{dimension.width.small.value}"
            }
        }
    }
}
