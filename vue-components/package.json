{
  "name": "@wmde/wikit-vue-components",
  "version": "2.1.0-alpha.11",
  "description": "The component implementation of WiKit in vue",
  "author": {
    "name": "The Wikidata team"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --formats commonjs --name wikit-vue-components src/main.ts --report --report-json && tsc --emitDeclarationOnly && VUE_APP_VUE3COMPAT=true vue-cli-service build --target lib --formats commonjs --name wikit-vue-components-vue3compat --no-clean src/main.ts",
    "test:unit": "vue-cli-service test:unit",
    "test:a11y": "jest --testMatch=\"**/tests/a11y/**\"",
    "e2e": "vue-cli-service test:e2e",
    "e2e:saucelabs": "env DATE=\"$(date)\" nightwatch --config nightwatch.config.js --env sauceChrome,sauceFirefox,sauceEdge,sauceSafari",
    "build:extract-stories": "npx sb extract storybook-static storybook-static/stories.json",
    "build:storybook": "build-storybook",
    "fix": "eslint --ext .js,.ts,.vue --fix .",
    "fix:css": "stylelint '**/*.vue' --fix",
    "storybook": "start-storybook",
    "test": "npm-run-all test:*",
    "test:lint": "eslint --ext .js,.ts,.vue --max-warnings 0 .",
    "test:lintcss": "stylelint '**/*.vue'",
    "test-unit-coverage": "vue-cli-service test:unit -c ./jest.config.js --coverage",
    "chromatic": "npx chromatic build --project-token=3evi132fpys --build-script-name=build:storybook",
    "prepare": "npm run build"
  },
  "main": "dist/wikit-vue-components.common.js",
  "files": [
    "src/",
    "dist/"
  ],
  "types": "dist/main.d.ts",
  "dependencies": {
    "@wmde/wikit-tokens": "^2.1.0-alpha.11",
    "core-js": "^3.7.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "ress": "^3.0.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-a11y": "~6.4.18",
    "@storybook/addon-backgrounds": "~6.4.18",
    "@storybook/addon-controls": "~6.4.18",
    "@storybook/addon-docs": "~6.4.18",
    "@storybook/addon-viewport": "~6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/vue3": "~6.4.18",
    "@types/jest": "^27.0.0",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash": "^4.14.165",
    "@types/lodash.isequal": "^4.5.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vue/cli-plugin-babel": "^5",
    "@vue/cli-plugin-e2e-nightwatch": "^5",
    "@vue/cli-plugin-eslint": "^5",
    "@vue/cli-plugin-typescript": "^5",
    "@vue/cli-plugin-unit-jest": "^5",
    "@vue/cli-service": "^5",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@wmde/eslint-config-wikimedia-typescript": "0.2.2",
    "chromatic": "^6.4.3",
    "chromedriver": "83",
    "eslint": "^8.10.0",
    "eslint-config-wikimedia": "0.21.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-plugin-vue-root-class": "wmde/eslint-plugin-vue-root-class#bumpCompatibility",
    "geckodriver": "^1.19.1",
    "jest-axe": "^4.1.0",
    "jest-serializer-vue": "^2.0.2",
    "nightwatch": "^2.0.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-dir-pseudo-class": "^5.0.0",
    "postcss-inline-svg": "^4.1.0",
    "postcss-logical": "^4.0.2",
    "postcss-selector-parser": "^6.0.4",
    "sass": "^1.29.0",
    "sass-loader": "^9.0.3",
    "saucelabs": "^4.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-use-logical-spec": "^3.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "~4.5.0",
    "vue-eslint-parser": "^8.3.0"
  },
  "homepage": "https://github.com/wmde/wikit/tree/master/vue-components#readme",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/wikit.git"
  },
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/wikidata_design_system/"
  }
}
