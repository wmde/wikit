{
  "name": "@wmde/wikit-vue-components",
  "version": "2.0.0-alpha.0",
  "description": "The component implementation of WiKit in vue",
  "author": {
    "name": "The Wikidata team"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --formats commonjs --name wikit-vue-components src/main.ts --report --report-json && tsc --emitDeclarationOnly",
    "test:unit": "vue-cli-service test:unit",
    "e2e": "vue-cli-service test:e2e",
    "e2e:saucelabs": "env DATE=\"$(date)\" nightwatch --config nightwatch.config.js --env sauceChrome,sauceFirefox,sauceIE,sauceEdge,sauceSafari",
    "build:extract-stories": "npx sb extract storybook-static storybook-static/stories.json",
    "build:storybook": "build-storybook",
    "fix": "vue-cli-service lint",
    "fix:css": "stylelint '**/*.vue' --fix",
    "storybook": "start-storybook",
    "test": "npm-run-all test:*",
    "test:lint": "vue-cli-service lint --no-fix",
    "test:lintcss": "stylelint '**/*.vue'",
    "test-unit-coverage": "vue-cli-service test:unit -c ./jest.config.js --coverage",
    "chromatic": "npx chromatic build --project-token=3evi132fpys --build-script-name=build:storybook",
    "prepare": "npm run build"
  },
  "main": "dist/wikit-vue-components.common.js",
  "files": [
    "src/",
    "dist/"
  ],
  "types": "dist/main.d.ts",
  "dependencies": {
    "@wmde/wikit-tokens": "^2.0.0-alpha.0",
    "core-js": "^3.7.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "ress": "^3.0.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "~6.0.28",
    "@storybook/addon-backgrounds": "~6.0.28",
    "@storybook/addon-controls": "~6.0.28",
    "@storybook/addon-docs": "~6.0.28",
    "@storybook/addon-viewport": "~6.0.28",
    "@storybook/vue": "~6.0.28",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.165",
    "@types/lodash.isequal": "^4.5.5",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-e2e-nightwatch": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "^1.1.1",
    "@wmde/eslint-config-wikimedia-typescript": "^0.2.0",
    "chromatic": "^5.4.0",
    "chromedriver": "83",
    "eslint": "^7.13.0",
    "eslint-config-wikimedia": "^0.16.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-root-class": "^0.1.0",
    "geckodriver": "^1.19.1",
    "jest-serializer-vue": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-dir-pseudo-class": "^5.0.0",
    "postcss-logical": "^4.0.2",
    "postcss-selector-parser": "^6.0.4",
    "sass": "^1.29.0",
    "sass-loader": "^9.0.3",
    "saucelabs": "^4.5.2",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-use-logical": "^1.1.0",
    "typescript": "^3.9.7",
    "vue-template-compiler": "^2.6.12"
  },
  "homepage": "https://github.com/wmde/wikit/tree/master/vue-components#readme",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/wikit.git"
  },
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/wikidata_design_system/"
  }
}
