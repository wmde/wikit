{
  "name": "@wmde/wikit-vue-components",
  "version": "0.0.0",
  "description": "The component implementation of wikit in vue",
  "author": {
    "name": "The Wikidata team"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --formats commonjs --name wikit-vue-components src/main.ts --report --report-json",
    "test:unit": "vue-cli-service test:unit",
    "test:lint": "vue-cli-service lint --no-fix",
    "test": "npm-run-all test:*",
    "fix": "vue-cli-service lint",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook"
  },
  "main": "dist/index.js",
  "files": [
    "src/",
    "dist/"
  ],
  "dependencies": {
    "@wmde/wikit-tokens": "^0.0.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.0.0-rc.24",
    "@storybook/addon-docs": "^6.0.0-rc.24",
    "@storybook/addon-knobs": "^6.0.0-rc.24",
    "@storybook/vue": "^6.0.0-rc.24",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "@wmde/eslint-config-wikimedia-typescript": "^0.2.0",
    "eslint": "^7.2.0",
    "eslint-config-wikimedia": "^0.16.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-prefixwrap": "^1.16.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "homepage": "https://github.com/wmde/wikit/tree/master/vue-components#readme",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/wikit.git"
  }
}
